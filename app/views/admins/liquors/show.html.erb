 <div class='col-md-9 offset-md-2'>
  <h2>お酒詳細</h2>
    <div class='row'>
     <div class='col-md-1'>
      <%= attachment_image_tag @liquor, :image, fallback: "no_image.jpg", size: "300x250" %>
     </div>
     <div class='col-md-8'>
      <table class="table table-borderless">
       <tr>
      	  <th>お酒名</th>
      	  <td><%= @liquor.name %></td>
     	 </tr>
       <tr>
      	  <th>お酒説明</th>
      	  <td><%= @liquor.detail %></td>
       </tr>
       <tr>
      	  <th>ジャンル</th>
      	  <td><%= @liquor.genre.name %></td>
       </tr>
      </table>
     </div>
    </div>
  <%= link_to "編集する", edit_admins_liquor_path(@liquor), class: "btn btn-success btn-lg" %>
 </div>